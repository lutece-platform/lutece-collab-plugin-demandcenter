<?xml version="1.0" encoding="UTF-8"?>
<plug-in>
    <name>demandcenter</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>demandcenter.plugin.description</description>
    <provider>demandcenter.plugin.provider</provider>
    <provider-url>http://your.web.site.com</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2016 Your Company</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>DEMANDCENTER_MANAGEMENT</feature-id>
            <feature-title>demandcenter.adminFeature.ManageDemandcenter.name</feature-title>
            <feature-description>demandcenter.adminFeature.ManageDemandcenter.description</feature-description>
            <feature-level>1</feature-level>
            <feature-url>jsp/admin/plugins/demandcenter/ManageCategorys.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
      </admin-feature>
      <admin-feature>
            <feature-id>DEMANDS_MANAGEMENT</feature-id>
            <feature-title>demandcenter.adminFeature.ManageDemands.name</feature-title>
            <feature-description>demandcenter.adminFeature.ManageDemands.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/demandcenter/ManageDemands.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
      </admin-feature>
      <admin-feature>
            <feature-id>USER_PREFERENCES_MANAGEMENT</feature-id>
            <feature-title>demandcenter.adminFeature.ManageUserPreferences.name</feature-title>
            <feature-description>demandcenter.adminFeature.ManageUserPreferences.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/demandcenter/UserPreferences.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
    </admin-features>    
    
    <admin-css-stylesheets>
        <admin-css-stylesheet>css/admin/plugins/demandcenter/demandcenter.css</admin-css-stylesheet>
    </admin-css-stylesheets>

    <!-- XPages parameters -->
    <applications>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
   </portlets>
   
   <!-- RBAC Resources -->
    <rbac-resource-types>
        <rbac-resource-type>
            <rbac-resource-type-class>
                fr.paris.lutece.plugins.demandcenter.service.category.CategoryIdService
            </rbac-resource-type-class>
        </rbac-resource-type>
    </rbac-resource-types>
</plug-in>
